theme = tokyonight-storm

keybind = ctrl+a>t=new_tab
keybind = ctrl+a>n=next_tab
keybind = ctrl+a>p=previous_tab
keybind = ctrl+a>s=new_split:right
keybind = ctrl+a>v=new_split:down
keybind = ctrl+a>j=goto_split:top
keybind = ctrl+a>h=goto_split:previous
keybind = ctrl+a>k=goto_split:bottom
keybind = ctrl+a>l=goto_split:next
